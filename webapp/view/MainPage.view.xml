                    placeholder="Select Your Restaurant"
                    visible="true"
                    id="SelectYourRestaurant"
                    items="{
			            	path: 'Data1>/results',
				            sorter: { path: 'Name1' } }"
                >
                    <core:Item
                        id="idcoreitem"
                        text="{Data1>Name1}"
                    />
                </ComboBox>

                <Button
                    text="FIND FOOD"
                   
                    press="onFindFood"
                />
                <ToolbarSpacer />
                <SearchField
                    liveChange="onSearch"
                    width="30%"
                />
               
