        <headerContent >
          <Button
            icon="sap-icon://customer" class="findfoodBtn btnnn"
                    width="7rem"
                    press="oncustomer" visible="{=${Data2>/CustomerID} === 'V' }" 
                />
                  <Button
            icon="sap-icon://customer" class="findfoodBtn btnnn"
                    width="7rem"
                    press="onCustomerDatails" visible="{=${Data2>/CustomerID} !== 'V' }" 
